.block
  = render :partial => 'shared/breadcrumbs', :locals => {:parent => @course}
  .content
    %h2.title=t('documents.edit')
    .inner
      = simple_form_for(resource, :url => resource_url, :html => { :class => :form, :multipart => true }) do |f|
        = render :partial => "form", :locals => {:f => f}  
